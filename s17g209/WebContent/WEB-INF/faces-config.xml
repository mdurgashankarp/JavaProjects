<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<managed-bean>
		<managed-bean-name>dbInformationBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.DbInformationBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dbAccessBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.DbAccessBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dbAccessActionBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.DbAccessActionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>messagesBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.MessagesBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionBeanComputeReturns</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.ActionBeanComputeReturns</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>statisticsActionBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.StatisticsActionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>statisticsBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.StatisticsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actionBeanAnalysis</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.ActionBeanAnalysis</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
<!-- 	<managed-bean>
		<managed-bean-name>fileActionBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.FileActionBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean> -->
	<managed-bean>
		<managed-bean-name>regressionAnalysisManagedBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids517.model.RegressionAnalysisManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Failure</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/logout.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/index.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/accesslog.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/computereturn.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>HOME</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/databaseProperties.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>HOME</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/exportFile.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/generateStatistics.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>HOME</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/graphAnalysis.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>HOME</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/home.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/importFile.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>HOME</from-outcome>
			<to-view-id>/home.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
